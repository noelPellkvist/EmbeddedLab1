import ForSyDe.Shallow



main :: IO ()
main = do 
  let s_in   = takeS 10 $ infiniteS (+1) 0
  let s_bool = signal [True, True, True, True, True, False, True, True, True, True]
  let (s_out_1, s_out_2) = System s_in s_bool
  putStrLn "=== s_out_1 ==="
  print s_out_1
  putStrLn "=== s_out_2 ==="
  print s_out_2
